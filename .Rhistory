per <- pretty_round(num / denom * percent_scaling, digits, ...)
if (as_per) {
per_delim <- "% ("
} else {
per_delim <- " ("
}
per_as_char <- paste0(per, per_delim, num, "/", denom, ")")
return(per_as_char)
}
