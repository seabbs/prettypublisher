pretty_percentage <- function(num, denom, digits = 2, percent_scaling = 100, ...) {
per <- pretty_round(num / denom * percent_scaling, digits, as_per = TRUE ...)
if (as_per) {
per_delim <- "% ("
} else {
per_delim <- " ("
}
per_as_char <- paste0(per, per_delim, num, "/", denom, ")")
return(per_as_char)
}
